{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "settings",
    "description": "Capability for the settings window",
    "windows": [
        "settings"
    ],
    "permissions": [
        "core:window:allow-close",
        "core:event:default",
        "core:app:allow-set-app-theme",
        "core:webview:allow-internal-toggle-devtools",
        "store:default"
    ]
}
