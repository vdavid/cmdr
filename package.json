{
    "name": "cmdr",
    "private": true,
    "packageManager": "pnpm@10.30.0",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "RUST_LOG=${RUST_LOG:-smb=warn,sspi=warn,info} pnpm --filter @cmdr/desktop tauri dev",
        "dev:ai-debug": "CMDR_REAL_AI=1 RUST_LOG=cmdr_lib::ai=debug,smb=warn,sspi=warn,info pnpm --filter @cmdr/desktop tauri dev",
        "build": "pnpm --filter @cmdr/desktop tauri build",
        "dev:website": "pnpm --filter @cmdr/website dev",
        "build:website": "pnpm --filter @cmdr/website build",
        "dev:license-server": "pnpm --filter @cmdr/license-server dev",
        "deploy:license-server": "pnpm --filter @cmdr/license-server deploy",
        "check": "./scripts/check.sh"
    }
}